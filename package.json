{
  "name": "abrasoft-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npx eslint \"**/*.{js,ts,tsx}\" --fix && npx stylelint \"**/*.{css,scss}\" --fix && npx prettier \"**/*.{html,json}\" --write",
    "serve": "npm run build && npm run start",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged && npx next build\""
  },
  "dependencies": {
    "@vercel/postgres": "^0.5.1",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "next": "^14.0.3",
    "react": "^18",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18",
    "react-icons": "^4.11.0",
    "sass": "^1.69.5",
    "sharp": "^0.33.0",
    "use-debounce": "^10.0.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.4.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-next": "13.5.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-stylelint": "^20.0.0",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.1.0",
    "stylelint-scss": "^5.3.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix",
    "*.{html,json}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npx next build"
    }
  }
}
